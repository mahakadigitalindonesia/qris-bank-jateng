{
  "name": "mdigi/qris-bank-jateng",
  "description": "Qris API Bank Jateng",
  "keywords": [
    "mdigi",
    "qris-bank-jateng"
  ],
  "license": "Apache-2.0",
  "type": "library",
  "authors": [
    {
      "name": "Farhan",
      "email": "farhan@mdigi.tech",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^9.0"
  },
  "require-dev": {
    "orchestra/testbench": "^7.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Mdigi\\QrisBankJateng\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mdigi\\QrisBankJateng\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Mdigi\\QrisBankJateng\\QrisBankJatengServiceProvider"
      ]
    }
  }
}
